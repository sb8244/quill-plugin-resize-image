{
  "name": "quill-image-resizer-plugin",
  "version": "1.0.0",
  "description": "A module for Quill rich text editor to allow images to be resized.",
  "main": "./build/dist/index.js",
  "module": "./build/dist/index.js",
  "types": "./build/types/index.d.ts",
  "files": [
    "build/dist/modules/",
    "build/dist/index.js",
    "build/dist/ImageResize.js",
    "build/dist/DefaultOptions.js",
    "build/dist/icons/",
    "build/types/",
    "build/_snowpack/"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/vitting/quill-plugin-resize-image"
  },
  "keywords": [
    "quill",
    "image",
    "resize",
    "sizer"
  ],
  "license": "MIT",
  "scripts": {
    "start": "snowpack dev",
    "build": "snowpack build",
    "postbuild": "tsc --declaration --emitDeclarationOnly --declarationDir build/types",
    "run:declarations": "tsc --declaration --emitDeclarationOnly --declarationDir build/types",
    "format": "prettier --write \"src/**/*.{ts,js}\"",
    "lint": "prettier --check \"src/**/*.{ts,js}\""
  },
  "devDependencies": {
    "@snowpack/plugin-typescript": "^1.2.1",
    "@types/lodash": "^4.14.168",
    "@types/quill": "^2.0.5",
    "@types/snowpack-env": "^2.3.3",
    "prettier": "^2.2.1",
    "snowpack": "^3.0.11",
    "snowpack-plugin-raw": "^1.0.0",
    "typescript": "^4.1.5"
  },
  "dependencies": {
    "lodash": "^4.17.20",
    "quill": "^1.3.7"
  }
}
